<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TechnologyRatingValidation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(TEXT(Technology_Rating__c),&quot;([1-5]{1})&quot;))</errorConditionFormula>
    <errorDisplayField>Technology_Rating__c</errorDisplayField>
    <errorMessage>Put Numbers between 1 to 5</errorMessage>
</ValidationRule>
